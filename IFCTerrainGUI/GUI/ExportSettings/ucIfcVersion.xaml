<UserControl x:Class="IFCTerrainGUI.GUI.ExportSettings.ucIfcVersion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IFCTerrainGUI.GUI.ExportSettings"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="380">
    <Grid>
        <StackPanel Grid.Column="1" Orientation="Horizontal">
            <ComboBox Width="100" VerticalAlignment="Center">
                <!--default seeting GCS-->
                <ComboBoxItem Content="IFC 2x3 (MESH)"/>
                <ComboBoxItem Content="IFC 4 (MESH)"/>
                <ComboBoxItem Content="IFC 2x3 (TIN)"/>
                <ComboBoxItem Content="IFC 4 (TIN)" IsSelected="True"/>
                <ComboBoxItem Content="IFC 4.3" IsEnabled="False"/>
            </ComboBox>
            <CheckBox VerticalAlignment="Center" Content="IfcXML" Margin="7,0,0,0" ToolTip="Check if you want to export Ifc-XML files."/>
            <CheckBox VerticalAlignment="Center" Content="IfcGeographicElement" Margin="7,0,0,0"/>
        </StackPanel>
    </Grid>
</UserControl>
