<Window x:Class="City2RVT.GUI.Properties.Wpf_CreateMetaJSON"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:City2RVT.GUI.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="400"
        Height="450" Width="400" Title="Select theme">
    <Grid>
        <ComboBox Name="Cb_Theme" HorizontalAlignment="Left" Margin="136,104,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="Cb_Theme_SelectionChanged"
