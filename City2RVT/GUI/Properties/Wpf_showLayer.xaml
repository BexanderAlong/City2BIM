<Window x:Class="City2RVT.GUI.Properties.Wpf_showLayer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:City2RVT.GUI.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
        Height="550" Width="400" Title="Select layer">
    <Grid>
        <ListBox x:Name="lb_layer" HorizontalAlignment="Left" Height="450" Margin="26,25,0,0" VerticalAlignment="Top" Width="335" SelectionChanged="lb_layer_SelectionChanged"
                 MouseDoubleClick="mdc_layer"/>

    </Grid>
</Window>
